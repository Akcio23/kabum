{
    "version": 2,
    "builds": [
      {
        "src": "app/**/*",  
        "use": "@vercel/next"
      },
      {
        "src": "index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/contato",  
        "dest": "/index.js"
      },
      {
        "src": "/(.*)", 
        "dest": "/"
      }
    ]
  }
  